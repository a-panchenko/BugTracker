<weblogic-web-app xmlns="http://xmlns.oracle.com/weblogic/weblogic-web-app">
    <security-role-assignment>
        <role-name>valid-user</role-name>
        <principal-name>administrators</principal-name>
        <principal-name>project-managers</principal-name>
        <principal-name>debugers</principal-name>
        <principal-name>testers</principal-name>
    </security-role-assignment>

    <security-role-assignment>
        <role-name>administrator</role-name>
        <principal-name>administrators</principal-name>
    </security-role-assignment>

    <security-role-assignment>
        <role-name>project-manager</role-name>
        <principal-name>project-managers</principal-name>
    </security-role-assignment>

    <security-role-assignment>
        <role-name>debuger</role-name>
        <principal-name>debugers</principal-name>
    </security-role-assignment>

    <security-role-assignment>
        <role-name>tester</role-name>
        <principal-name>testers</principal-name>
    </security-role-assignment>

</weblogic-web-app>